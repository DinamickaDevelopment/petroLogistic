//============================= Home Page Value Points  ========================================
.valuePoints
	.sectionHeader
		text-align:center;
		h3 
			font-weight:600;
			font-family:"Open Sans";
			text-align:center;
			color:#1991eb;
		p
			padding: 0 10px;
			font:300 16px/18px "Open Sans";  
			color:#909090;
			margin: 0 0 20px 0;
	.sectionContent
		color:#2b2b2b;
		for i in 1 2 3 4
			.col-lg-3:nth-child({i})
				.circle
					.circleImg
						background: url("/images/spriteHomePage.png") no-repeat (i - 1) * -66px   0px;
		.circle 
			width:130px;
			height:130px;
			border:2px solid #2b2b2b;
			border-radius:100%;
			margin:0 auto;
			background: #dcdce6;
			position:relative;
			
			.circleImg
				width:66px;
				height:66px;
				position:absolute;
				margin: -33px 0 0 -33px;
				left: 50%;
				top: 50%;
		div.underCircleText
			font:300 16px/24px "Open Sans"; 
			margin:10px 0 30px 0;
			text-align:center;
			
