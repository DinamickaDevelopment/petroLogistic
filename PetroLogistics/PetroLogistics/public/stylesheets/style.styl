	.sectionContent
		color:#2b2b2b;
		for i in 1 2 3 4 5 6 7 8 9
			.col-lg-4:nth-child({i})
				.circle
					.circleImg
						background: url("/images/spriteHomePage.png") no-repeat (i - 1) * -66px  -66px;
						width:66px;
						height:66px;
						position:absolute;
						margin: -33px 0 0 -33px;
						left: 50%;
						top: 50%;
		.circle
			position:relative; 
			width:66px;
			height:66px;
			margin:0 auto;
		div.underCircleText
			font:100 20px "Open Sans"; 
			margin:10px 0 30px 0;
			text-align:center;

