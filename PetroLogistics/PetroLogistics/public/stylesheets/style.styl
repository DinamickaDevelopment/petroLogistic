//============================= Footer ========================================

footer
	font:100 14px/18px "Open Sans";
	
	background:#2b2b2b;
	span 
		color:#fafafa;
		display:inline-block;
		margin:21px 0;
	a 
		display:inline-block;
		text-decoration:underline;
		text-align:center;
		cursor:pointer;
		color:#fafafa;
		margin:21px 0;
	a:hover 
		color:#fafafa;

@media (min-width: 320px) and (max-width: 767px)		
	footer
		text-align:center;

//============================= Home Page Request Trial  ========================================
section.reqTrial
	background:url("/images/home_bg.png") no-repeat;
	background-size:cover;
	position:relative;
	h1 
		text-transform:uppercase;
	a.req 
		width:165px;
		height:40px;
		padding:0!important;
		margin:45px 0 0 0;
		background-color:#fafafa !important;
		color: #2b2b2b;
		border:1px solid #fafafa !important;
		border-radius:0!important;
		font:600 16px/40px "Open Sans";
		text-transform:uppercase;
	a.req:hover 
		background:#1991eb !important;
		color:#fafafa!important;
	a.moveLink
		display: block;
		width: 50px;
		height: 50px;
		position: absolute;
		top:inherit;
		bottom:52px;
		left:50%;
		margin:0 0 0 -25px;
		background: #fafafa;
		color:#1991eb;
		border:1px solid #fafafa; 
		border-radius: 100%;
		cursor:pointer;
	a.moveLink:before
		font-size: 30px;
		width: 30px;
		height: 30px;
		position: absolute;
		margin: -15px 0 0 -15px;
		top: 50%;
		left: 50%;
		font-weight: 100;
	a.moveLink:hover 
		background:#1991eb;
		color:#fafafa;

@media (min-width: 320px) and (max-width: 374px)
	section.reqTrial
		h1
			font: 900 16px/24px "Open Sans";
			padding: 0 0 0 0;
